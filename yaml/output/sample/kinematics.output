# @author Sho Miyahara 2018

result:
  composite data name: cmp
  individual data name: ind

gp:
  check: false
  sampling time: 10
  start time offset: 0.

tex:
  title: Simulation results
  layout:
    -
      file name: mix
      caption: Mixed quasi values.
      elements:
        -
          file name: comPos
          dat value:
            vertical: time
            horizontal:
              terget: skeletonA7prim
              name: tree model rC
          y label: CoM pos. [mm]
          unit: m
          dimention: 3

        -
          file name: baseOrient
          dat value:
            vertical: time
            horizontal:
              terget: skeletonA7prim
              name: tree model xiB
          y label: Base orient. [deg]
          unit: RAD2DEG
          dimention: 3
    -
      file name: baseRotDes
      caption: Desired base rotation values.
      elements:
        -
          file name: baseOrientDes
          dat value:
            vertical: time
            horizontal:
              terget: 0
              name: xiBDes
          y label: Des. base orient. [deg]
          unit: RAD2DEG
          dimention: 3

gif:
  sampling time: 100
  link color: grey
  joint color: black
  out:
    terminal: true
    dir: true
  plane:
    y-z:
      x min: -0.5
      x max: 0.5
      y min: -0.5
      y max: 0.5
    x-z:
      x min: -0.5
      x max: 0.5
      y min: -0.5
      y max: 0.5
    x-y:
      x min: -0.5
      x max: 0.5
      y min: -0.5
      y max: 0.5
    3D:
      x min: -0.5
      x max: 0.5
      y min: -0.5
      y max: 0.5
      z min: -0.5
      z max: 0.5

choreonoid:
  sampling time: 10
