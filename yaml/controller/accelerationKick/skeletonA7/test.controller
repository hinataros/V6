# @author Sho Miyahara 2018

angle unit: degree

controller:
  control model: skeletonA7
  input: torque

  default:
    control method:
      inverse dynamics controller: fullDynamicsController

      operational space controller: decomposeEndEffectorAcceleration
      motion controller: mixedGeneralizedMomentum

      momentum controller: centroidalVrpMomentum
      force controller: centroidalEcmpDistribution

      torque controller: mixedmixedOpt

    desired value generator:
      DCM: {name: CP}
      base rotation: {name: CP}
      control node translation:
        RARMEE: {num: 0, name: CP}
        LARMEE: {num: 1, name: CP}
      control node rotation:
        RARMEE: {num: 0, name: CP}
        LARMEE: {num: 1, name: CP}

    Bc:
      RLEGEE: [1, 1, 1, 1, 1, 1]
      LLEGEE: [1, 1, 1, 1, 1, 1]

    Bm:
      RARMEE: [1, 1, 1, 1, 1, 1]
      LARMEE: [1, 1, 1, 1, 1, 1]

    KX: 3.e+2

    KpwB: 3.e+2
    KdwB: 5.e+1

    Kpv:
      RARMEE: 3.e+2
      LARMEE: 3.e+2
    Kdv:
      RARMEE: 5.e+1
      LARMEE: 5.e+1

    Kpw:
      RARMEE: 3.e+2
      LARMEE: 3.e+2
    Kdw:
      RARMEE: 5.e+1
      LARMEE: 5.e+1

    KDth: 1.e+2

  state:
    -

  sequence:
    -
      -
        twf: 1.
      -
        twf: 2.
        xiBf: [0., 30., 0.]
      -
        twf: 1.
        rf:
          RARMEE: [0., 0., 0.1]
    -
      -
        twf: 1.
      -
        twf: 3.
        rf:
          LARMEE: [0.1, 0., 0.1]
        xif:
          LARMEE: [0., 60., 90.]
