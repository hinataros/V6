# @author Ryotaro Hinata 2019

angle unit: degree

controller:
  control model: skeletonA4W1
  input: torque

  default:
    control method:
      inverse dynamics controller: highGainController
      operational space controller: relativeVelocityMixedMap
      motion controller: mixedVelocitySynergy

    desired value generator:
      CoM: {name: CP}
      base rotation: {name: CP}
      control node translation:
        RLEGEE: {name: CP}
        LLEGEE: {name: CP}
        RARMEE: {name: CP}
        LARMEE: {name: CP}
      control node rotation:
        RLEGEE: {name: CP}
        LLEGEE: {name: CP}
        RARMEE: {name: CP}
        LARMEE: {name: CP}

    Bc:
      RLEGEE: [1, 1, 1, 1, 1, 1]
      LLEGEE: [1, 1, 1, 1, 1, 1]
      RARMEE: [0, 0, 0, 0, 0, 0]
      LARMEE: [0, 0, 0, 0, 0, 0]
    Bm:
      RLEGEE: [0, 0, 0, 0, 0, 0]
      LLEGEE: [0, 0, 0, 0, 0, 0]
      RARMEE: [0, 0, 0, 0, 0, 0]
      LARMEE: [0, 0, 0, 0, 0, 0]

    KpC: 1.e+2
    KpwB: 1.e+2
    # Kpv:
    #   RARMEE: 3.e+2
    #   LARMEE: 3.e+2

    KpHG: 1.e+2
    KdHG: 1.e+1

  sequence:
    -
      -
        twf: 1.0
      -
        twf: 2.
        KDth: 1.0
        control method:
          ext motion controller: true
          motion controller: RAV_noDistribution

      -
        twf: 1.
        KDth: 0
        rCf: [0., 0.02, 0.]

      -
        twf: 1.
        Bc:
          RLEGEE: [0, 0, 0, 0, 0, 0]
          LLEGEE: [1, 1, 1, 1, 1, 1]
          RARMEE: [0, 0, 0, 0, 0, 0]
          LARMEE: [0, 0, 0, 0, 0, 0]
          
        Bm:
          RLEGEE: [1, 1, 1, 1, 1, 1]
          LLEGEE: [0, 0, 0, 0, 0, 0]
          RARMEE: [0, 0, 0, 0, 0, 0]
          LARMEE: [0, 0, 0, 0, 0, 0]
        rf:
          RLEGEE: [0., 0., 0.01]

      -
        twf: 0.3
        rf:
          RLEGEE: [-0.1, 0., 0.1]
        xif:
          RLEGEE: [0., 60, -15]
      -
        twf: 0.3
        Kpv:
          RLEGEE: 1.e+1
          LLEGEE: 1.e+1
        Kpw:
          RLEGEE: 1.e+1
          LLEGEE: 1.e+1

      -
        twf: 0.3
        desired value generator:
          control node translation:
            RLEGEE: {num: 0, name: circle}
        rf:
          RLEGEE: [0.1, 0., 0.03]
        xif:
          RLEGEE: [0., -10, -20]
