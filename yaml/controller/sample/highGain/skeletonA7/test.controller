# @author Sho Miyahara 2018

angle unit: degree

controller:
  control model: skeletonA7
  input: torque

  default:
    control method:
      inverse dynamics controller: highGainController
      operational space controller: relativeVelocityMixedMap
      motion controller: mixedVelocitySynergy

    desired value generator:
      CoM: {name: CP}
      base rotation: {name: CP}
      control node translation:
        RARMEE: {name: CP}
        LARMEE: {name: CP}
      control node rotation:
        RARMEE: {name: CP}
        LARMEE: {name: CP}

    Bc:
      RLEGEE: [1, 1, 1, 1, 1, 1]
      LLEGEE: [1, 1, 1, 1, 1, 1]
    Bm:
      RARMEE: [1, 1, 1, 1, 1, 1]
      LARMEE: [1, 1, 1, 1, 1, 1]

    KpC: 3.e+2
    KpwB: 3.e+2

    Kpv:
      RARMEE: 3.e+2
      LARMEE: 3.e+2
    Kpw:
      RARMEE: 3.e+2
      LARMEE: 3.e+2

    KpHG: 1.e+2
    KdHG: 1.e+1

  sequence:
    -
      twf: 1.
    -
      twf: 2.
      rCf: [0., -0.02, 0.]
      xiBf: [0., 0.32, 0.]
    -
      twf: 1.
      rf:
        RARMEE: [0.1, 0., 0.1]
      xif:
        RARMEE: [0., 60., 90.]
