# @author Sho Miyahara 2018

Controller:
  Control model: skeletonA4
  Input: torque

  Default:
    Inverse dynamics controller: highGainController
    Operational space controller: relativeVelocityMixedMap
    Motion controller: mixedVelocitySynergy

    Desired CoM generator: {name: CP}
    Desired base orientation generator: {name: CP}
    Desired control node motion generator:
      RARMEE: {name: CP}
      LARMEE: {name: CP}

    Bc:
      RLEGEE: [1, 1, 1, 1, 1, 1]
      LLEGEE: [1, 1, 1, 1, 1, 1]
    Bm:
      RARMEE: [1, 1, 1, 0, 0, 0]
      LARMEE: [1, 1, 1, 0, 0, 0]

    KpC: 3.e+2
    KpwB: 3.e+2
    Kpv:
      RARMEE: 3.e+2
      LARMEE: 3.e+2

    KpHG: 1.e+2
    KdHG: 1.e+1

  Sequence:
    -
      twf: 2.
    -
      twf: 2.
      rCf: [0., 0., -0.03]
      xiBf: [0., 0.32, 0.]
    -
      twf: 1.
      cal_Xf:
        RARMEE: [0.1, 0., 0.1, 0., 0., 0.]
