# @author Sho Miyahara 2018

controller:
  control model: skeletonA4
  input: velocity
  # Driven: mix
  # Trigger: check contact

  default:
    Desired base orientation generator: {name: CP}
    Desired control node acceleration generator:
      RARMEE: {num: 0, name: CP}
      LARMEE: {num: 1, name: CP}

    Operational space controller: relativeVelocityBaseMap
    Motion controller: baseVelocitySynergy

    Bc:
      RLEGEE: [1, 1, 1, 1, 1, 1]
      LLEGEE: [1, 1, 1, 1, 1, 1]

    Bm:
      RARMEE: [1, 1, 1, 0, 0, 0]
      LARMEE: [1, 1, 1, 0, 0, 0]

    # KpC: .
    # KdC: .

    KpwB: 10.
    Kpv:
      RARMEE: 10.
      LARMEE: 10.

  state:
    -

  sequence:
    -
      -
        twf: 1.
        xiBf: [0., 0.52, 0.]
      -
        twf: 1.
        cal_Xf:
          RARMEE: [0., 0., 0.1, 0., 0., 0.]
    -
      -
        twf: 3.
        cal_Xf:
          LARMEE: [0.1, 0., 0.1, 0., 0., 0.]

