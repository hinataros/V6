# @author Sho Miyahara 2018

angle unit: degree

controller:
  control model: skeletonA4
  input: torque

  default:
    control method:
      inverse dynamics controller: highGainController
      operational space controller: relativeVelocityMixedMap
      motion controller: mixedVelocitySynergy

    desired value generator:
      CoM: {name: CP}
      base rotation: {name: CP}
      control node translation:
        RLEGEE: {name: CP}
        LLEGEE: {name: CP}
        RARMEE: {name: CP}
        LARMEE: {name: CP}
      control node rotation:
        RLEGEE: {name: CP}
        LLEGEE: {name: CP}
        RARMEE: {name: CP}
        LARMEE: {name: CP}

    Bc:
      RLEGEE: [1, 1, 1, 1, 1, 1]
      LLEGEE: [1, 1, 1, 1, 1, 1]
    Bm:
      RLEGEE: [0, 0, 0, 0, 0, 0]
      LLEGEE: [0, 0, 0, 0, 0, 0]

    KpC: 1.e+2
    KpwB: 1.e+2
    # Kpv:
    #   RARMEE: 3.e+2
    #   LARMEE: 3.e+2

    KpHG: 1.e+2
    KdHG: 1.e+1

  sequence:
    -
      -
        twf: 0.5
      -
        twf: 2.0
        xiBf: [180., 0., 0.]
      -
        twf: 2.0
        xiBf: [0., 0., 0.]
      -
        twf: 2.0
        xiBf: [-180., 0., 0.]
      -
        twf: 2.0
        xiBf: [0., 0., 0.]
